@model PropMan.Models.PropertyType
@{
    ViewBag.Title = "تایید حذف";
}

<h2>@ViewBag.Title</h2>
<hr/>

<div class="row">
    <h3 class="text-danger">
        آیا از حذف نوع ملک @Model.Name اطمینان دارید؟
    </h3>
</div>
<div class="row">
    @using (Html.BeginForm())
    {
        @Html.HiddenFor(m => m.Id)
        @Html.AntiForgeryToken()
        <button class="btn btn-danger btn-sm pull-right" type="submit">
            پاکش کن!
        </button>
        <a href="@Url.Action("Index")" class="btn btn-sm btn-default pull-right" style="margin-right:15px;">نه، منصرف شدم</a>
    }

</div>

